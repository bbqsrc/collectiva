    "query-string": "^3.0.0",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.2",
    "watchify": "^3.7.0",
    "webpack-dev-server": "^1.14.1"
  }
}
