{
  "name": "collectiva",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "./node_modules/webpack/bin/webpack.js --config ./webpack.config.js --progress --colors -p",
    "prestart": "node_modules/sequelize-cli/bin/sequelize db:migrate",
    "start": "node --harmony-destructuring ./bin/www",
    "pretest": "NODE_ENV=test node_modules/sequelize-cli/bin/sequelize db:migrate",
    "test": "npm run serverTests",
    "serverTests": "NODE_ENV=test mocha --harmony-destructuring -r spec/support/globals spec/backend/unit/**/*.js",
    "componentTests": "NODE_ENV=test node ./node_modules/karma-cli/bin/karma start",
    "preacceptanceTests": "npm run createAdmin",
    "acceptanceTests": "NODE_ENV=test PHANTOMJS_EXECUTABLE=./node_modules/phantomjs/bin/phantomjs ./node_modules/casperjs/bin/casperjs --web-security=true --log-level=debug --verbose test spec/acceptance/user_journey.js spec/acceptance/admin_journey.js --url=http://localhost:3000 --email=$ACCEPTANCE_EMAIL --password=$ACCEPTANCE_PASSWORD | grep -v 'Unsafe JavaScript attempt to access frame' | grep -v -e '^$'",
    "acceptanceTestsStaging": "NODE_ENV=test PHANTOMJS_EXECUTABLE=./node_modules/phantomjs/bin/phantomjs ./node_modules/casperjs/bin/casperjs --web-security=true --log-level=debug --verbose test spec/acceptance/user_journey.js spec/acceptance/admin_journey.js --url=https://collectiva-staging.herokuapp.com --email=$ACCEPTANCE_EMAIL --password=$ACCEPTANCE_PASSWORD | grep -v 'Unsafe JavaScript attempt to access frame' | grep -v -e '^$'",
    "bundle": "./node_modules/webpack/bin/webpack.js --config ./webpack.config.js --progress --colors --watch",
    "createAdmin": "node ./scripts/createAdmin.js"
  },
  "dependencies": {
    "alt": "^0.18.2",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "bcryptjs": "https://registry.npmjs.org/bcryptjs/-/bcryptjs-2.3.0.tgz",
    "bluebird": "^3.3.4",
    "body-parser": "~1.13.2",
    "braintree": "^1.37.1",
    "config": "^1.19.0",
    "connect-session-sequelize": "^3.0.0",
    "cron": "^1.1.0",
    "debug": "~2.2.0",
    "ejs": "^2.3.4",
    "express": "~4.13.1",
    "express-sanitized": "^0.5.1",
    "express-session": "^1.13.0",
    "handlebars": "^4.0.5",
    "helmet": "^1.0.2",
    "iso": "^5.1.0",
    "jquery": "^2.2.0",
    "koa": "^1.2.0",
    "koa-better-body": "^1.0.17",
    "koa-ejs": "^3.0.0",
    "koa-helmet": "^1.0.0",
    "koa-passport": "^1.3.0",
    "koa-rutt": "^0.3.0",
    "koa-session": "^3.3.1",
    "load-script": "^1.0.0",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "morgan": "~1.6.1",
    "node-neat": "^1.7.2",
    "node-sass-middleware": "0.8.0",
    "node-uuid": "^1.4.7",
    "nodemailer": "^2.3.0",
    "nodemailer-sendmail-transport": "^1.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "paypal-ipn": "^3.0.0",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "proxenv": "^1.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-modal": "^0.6.1",
    "readline": "^1.3.0",
    "scrypt": "^6.0.1",
    "sequelize": "^3.19.3",
    "sequelize-cli": "^2.2.1",
    "serve-favicon": "~2.3.0",
    "stripe": "^4.1.0",
    "underscore": "^1.8.3",
    "validator": "^4.4.0",
    "webpack": "^1.12.11",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "casperjs": "^1.1.0-beta3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "co-mocha": "^1.1.2",
    "core-js": "^2.0.3",
    "eslint": "^2.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.1.0",
    "jsx-loader": "^0.13.2",
    "karma": "^0.13.19",
    "karma-cli": "^0.1.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
